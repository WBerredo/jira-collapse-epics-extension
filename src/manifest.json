{
  "name": "Jira Collapse Epics",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["contentScript.bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon-64.png"
  },
  "icons": {
    "64": "icon-64.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
